{
	"tags": {
		"allowUnknownTags": true
	},
	"source": {
		"includePattern": ".+\\.js(doc)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"plugins": ["plugins/markdown"],
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true,
		"default": {
			"outputSourceFiles": false,
			"destination": "../../js/"
		},
		"applicationName": "Shelf JS Guide",
		"googleAnalytics": "",
		"openGraph": {
			"title": "Shelf JS Guide",
			"type": "website",
			"image": "",
			"site_name": "Shelf JS Guide",
			"url": "http://shelf.js/docs/js/"
		},
		"meta": {
			"title": "Shelf JS Guide",
			"description": "",
			"keyword": ""
		},
		"linenums": true
	},
	"markdown": {
		"parser": "gfm",
		"hardwrap": true,
		"tags": ["examples"]
	}
}